<%= render 'profile', :user => @user %>

<% if @user.user_type == "Teacher" %>
	<%= link_to "Book me now", new_user_event_path(@user) %>
	<%= calendar @events do |event| %>
	  <div><%= link_to event.name, root_path %></div>
	  <div><%= event.start_time.strftime("at %I:%M%p") %></div>
	<% end %>
<% end %>